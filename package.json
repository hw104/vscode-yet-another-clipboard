{
  "name": "yac",
  "displayName": "Yet Another Clipboard",
  "description": "Yet another clipboard history extension. Includes no keyboard shortcuts.",
  "version": "0.0.1",
  "publisher": "hw104",
  "repository": "https://github.com/hw104/vscode-yet-another-clipboard.git",
  "license": "MIT",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:yac.copy",
    "onCommand:yac.cut",
    "onCommand:yac.clear",
    "onCommand:yac.paste",
    "onCommand:yac.list"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "yac.copy",
        "title": "YAC: Copy"
      },
      {
        "command": "yac.cut",
        "title": "YAC: Cut"
      },
      {
        "command": "yac.clear",
        "title": "YAC: Clear"
      },
      {
        "command": "yac.paste",
        "title": "YAC: Paste"
      },
      {
        "command": "yac.list",
        "title": "YAC: Show history, paste or delete"
      }
    ],
    "configuration": {
      "title": "Yet Another Clipboard",
      "properties": {
        "yac.bufferSize": {
          "type": "integer",
          "default": 10,
          "description": "Maximum size of the history buffer.",
          "markdownDescription": "Maximum size of the history buffer."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "vs:package": "vsce package --yarn",
    "vs:publish": "vsce publish --yarn",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "vsce": "^2.10.0"
  }
}
